#include<iostream>

using namespace std;

const float PI = 3.14;
double S(double r);

double S(double r)
{
	double s;
	s = PI*r*r;
	return(s);
}

#include"area of circle.h"
#include<iostream>

using namespace std;

int main()
{
	cout << "请输入圆的半径：";
	double r;
	cin >> r;
	cout << "圆的面积为：" << S(r) << endl;
	return 0;
}
